{
  "buildCommand": "npm run build",
  "installCommand": "npm i --force --legacy-peer-deps",
  "regions": ["sin1"],
 "version": 2,
  "builds": [
    {
      "src": "./dist/main.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "dist/main.js"
    }
  ]
}