{
  "version": 2,
  "builds": [
    {
      "src": "dist/main.js",
      "use": "@vercel/node",
      "config": {
        "distDir": "dist"
      }
    }
  ],
  "crons": [
    {
    "path": "/api/attendance/cron/in",
    "schedule": "10 18 * * 1-5"
  },
    {
      "path": "/api/attendance/cron/out",
      "schedule": "30 10 * * 1-5"
    }
  ],
  "routes": [{
    "src": "/(.*)",
    "dest": "dist/main.js"
  }]
}