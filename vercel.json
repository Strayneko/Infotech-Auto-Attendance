{
  "buildCommand": "yarn build:vercel",
  "installCommand": "yarn",
  "routes": [
    {
      "src": "/(.*)",
      "dest": "dist/main.js",
      "methods": ["POST", "GET", "PATCH", "PUT"]
    }
  ],
  "regions": ["sin1"],
  "builds": [
    {
      "src": "dist/main.js",
      "use": "@vercel/node"
    }
  ]
}