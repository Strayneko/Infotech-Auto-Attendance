{
 "version": 2,
  "builds": [
    {
      "src": "./dist/main.js",
      "use": "@vercel/node",
      "config": {
        "buildCommand": "npm run build",
        "installCommand": "npm i --force --legacy-peer-deps",
        "regions": ["sin1"]
      }
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "dist/main.js"
    }
  ]
}